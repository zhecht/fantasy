<script type="text/javascript">
document.addEventListener("DOMContentLoaded", () => {
    const season = 2022;
    var positionFilter = document.querySelectorAll('.pos-filter li');
    var siteSelected = document.querySelector('.site-select');

    siteSelected.addEventListener('change', function(e){
        var selected = siteSelected?.options[siteSelected.selectedIndex]?.getAttribute('data-site');
        createCookie('fp_dfs_pref', selected, 365);
    });

    $('#data-table').tablesorter({
                    sortList: [[8,1]]
            });

    positionFilter.forEach(function(posFil, x){
        posFil.addEventListener('click', function(e){
            
            var posClick = posFil.getAttribute('data-pos');
            var posList = document.querySelectorAll('.pos-filter li');

            posList.forEach(function(clickPos, y){
                if(clickPos.classList.contains('active')){
                    clickPos.classList.remove('active');
                } else if(clickPos == posFil){
                    posFil.classList.add('active');
                }
            })

            FP.pageData.csvFilename = `FantasyPros_NBA_Defense_vs_Position_${posClick}_${season}.csv`;            

            filterTable();
        })
    })

    	document.querySelector('.export').addEventLister('click', function(e) {
		e.preventDefault();
		document.getElementById('premium-modal').modal('show');
	})
    
    document.querySelector('.game-change').addEventListener('change', function(e) {
        var game_class = e.target.value;
        if(game_class.includes('GC')){
            filterTable();
        } else {
            window.location.href = game_class;
        }
    })

    function filterTable() {

        var gameVal = document.querySelector('.game-change');
        var gameClass = gameVal.value;
        var pos = document.querySelector('.pos-filter li.active').getAttribute('data-pos');
        var tableData = document.querySelectorAll('#data-table tbody tr');
        if(gameClass == 'Games'){
            gameClass = 'GC-0';
        }
        tableData.forEach(function(tdata, y){
            if(tdata.classList.contains(pos) && tdata.classList.contains(gameClass)){
                tdata.style.display = '';
            } else {
                tdata.style.display = 'none';
            }
        })
    }
});
</script>

