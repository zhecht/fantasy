<script>
function UpdateThruSeason() {
			var fromseas = document.getElementById('fromseason');
			var thruseas = document.getElementById('thruseason');
			var fromindex = fromseas.selectedIndex;
			var thruindex = thruseas.selectedIndex;
			
			if (thruindex < fromindex) {
				thruindex = fromindex;
				document.getElementById('thruseason').selectedIndex = thruindex;
			} else if (thruindex - fromindex > 2) {
				thruindex = fromindex + 2;
				document.getElementById('thruseason').selectedIndex = thruindex;
			}
			
			for (i = 0; i < document.getElementById('thruseason').options.length; i++) { 
				if (i < fromindex) {
					document.getElementById('thruseason').options[i].disabled = 'disabled';
				} else if (i > fromindex + 2) {
					document.getElementById('thruseason').options[i].disabled = 'disabled';
				} else {
					document.getElementById('thruseason').options[i].disabled = '';
				}
			}
		}
</script>
