    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.6.0/jquery.min.js"></script>

            <link href="/frontend/assets/css/ckeditor-reset.css" rel="stylesheet" type="text/css">
        <script type="text/javascript" src="/frontend/assets/js/lineup-alerts.min.js?v=6"></script>    <script src="/frontend/assets/js/rotowire.min.js?v=163"></script>
    
    <script>
        $(document).ready(function(){
            const isSubscriber = $('.lineups').data('subscribed');

            $(document).on('click', '.minutes-viz__close', function(e){
                const $button = $(e.currentTarget);
                const $lineup = $button.parents('.lineup');

                $lineup.find('.minutes-viz').remove();
                $lineup.find('.lineup__box').removeClass('hide');
            });

            $(document).on('click', '.see-proj-minutes', async function(e){
                $('.fs-close-button-sticky').trigger('click');
                const $button = $(e.currentTarget);
                const team = $button.data('team');
                const nickname = $button.data('nickname');
                const isHome = $button.data('home');

                const $lineup = $button.parents('.lineup');
                const $lineupBox = $button.parents('.lineup__box');

                // Scroll up to the top of the lineup box if necessary
                //$lineup.get(0).scrollIntoView();

                // Hide the actual lineup
                $lineupBox.addClass('hide');

                // Add the minutes visualization container
                $lineup.append(`<div class="minutes-viz"></div>`);
                const $vizContainer = $lineup.find('.minutes-viz');

                const teamLogoSelector = isHome ? '.is-home .lineup__logo' : '.is-visit .lineup__logo';
                const teamLogoURL = $lineup.find(teamLogoSelector).attr('src');

                const teamColors = {
                    ATL: [359, 73, 55],
                    BKN: [0, 0, 0],
                    BOS: [152, 93, 27],
                    CHA: [249, 70, 22],
                    CHI: [345, 85, 44],
                    CLE: [341, 49, 29],
                    DAL: [205, 100, 36],
                    DEN: [216, 64, 15],
                    DET: [345, 86, 51],
                    GSW: [205, 100, 35],
                    HOU: [345, 85, 44],
                    IND: [212, 100, 19],
                    LAC: [346, 95, 57],
                    LAL: [271, 64, 34],
                    MEM: [220, 31, 51],
                    MIA: [342, 100, 30],
                    MIL: [146, 35, 22],
                    MIN: [213, 68, 15],
                    NOP: [222, 57, 23],
                    NYK: [27, 90, 56],
                    OKC: [202, 100, 38],
                    ORL: [203, 100, 38],
                    PHI: [205, 100, 36],
                    PHX: [36, 95, 55],
                    POR: [359, 73, 55],
                    SAC: [240, 45, 41],
                    SAS: [0, 0, 0],
                    TOR: [345, 85, 44],
                    UTA: [215, 66, 15],
                    WAS: [212, 100, 18],
                };

                const teamColor = teamColors[team];
                const teamColorHSL = `hsl(${teamColor[0]}deg ${teamColor[1]}% ${teamColor[2]}%)`;
                const teamBackgroundColorHSL = `hsl(${teamColor[0]}deg ${teamColor[1]}% 88%)`;
                const teamBackgroundLightColorHSL = `hsl(${teamColor[0]}deg ${teamColor[1]}% 97%)`;

                const topBar = `
                <div class="minutes-viz__top" style="background-color: ${teamBackgroundLightColorHSL};">
                    <img src="${teamLogoURL}" alt="${team}" class="minutes-viz__logo">
                    ${nickname}
                    <span>Projected Minutes</span>
                    <button class="minutes-viz__close" style="background-color: ${teamColorHSL};color: ${teamBackgroundLightColorHSL};">&times;</button>
                </div>`;


                $vizContainer.append(`
                    ${topBar}
                    <div class="minutes-viz-loader" style="background-color:${teamBackgroundLightColorHSL};">
                    <img src="${teamLogoURL}" alt="${team}" class="minutes-viz__logo">
                        <div style="margin-bottom:10px;">
                            <div class="minutes-meter" style="background-color: ${teamBackgroundColorHSL}"><div class="minutes-meter__proj" style="background-color:${teamColorHSL};">?</div></div>
                            <div class="minutes-meter" style="background-color: ${teamBackgroundColorHSL}"><div class="minutes-meter__proj" style="background-color:${teamColorHSL};animation-delay: 200ms;">?</div></div>
                            <div class="minutes-meter" style="background-color: ${teamBackgroundColorHSL}"><div class="minutes-meter__proj" style="background-color:${teamColorHSL};animation-delay: 400ms;">?</div></div>
                            <div class="minutes-meter" style="background-color: ${teamBackgroundColorHSL}"><div class="minutes-meter__proj" style="background-color:${teamColorHSL};animation-delay: 600ms;">?</div></div>
                            <div class="minutes-meter" style="background-color: ${teamBackgroundColorHSL}"><div class="minutes-meter__proj" style="background-color:${teamColorHSL};animation-delay: 800ms;">?</div></div>
                        </div>
                        <h2 style="color:${teamColorHSL}">Projecting Minutes...</h2>
                    </div>
                `);

                if (!isSubscriber && isHome) {
                    setTimeout(function(){
                        $vizContainer.find('.minutes-viz-loader').remove();

                        const paywall = `
                            <div class="minutes-viz-wall">
                                <div class="minutes-viz-wall__top">
                                    <div class="minutes-viz-wall__icon">
                                        <img src="/images/rw-square-dark-bg.svg" alt="RotoWire">
                                    </div>
                                    <div>
                                        <h2>Subscriber Exclusive</h2>
                                        <p>Projected minutes for this team are reserved for subscribers.</p>
                                    </div>
                                </div>
                                <p>If you'd like to see how this feature works, check out any visiting team (they are all free).</p>
                                <h3>Why subscribe to RotoWire?</h3>
                                <ul>
                                    <li>Minutes projections for every team</li>
                                    <li>Confirmed lineup alerts</li>
                                    <li>Projected stats and fantasy points</li>
                                    <li>In-depth defense vs. position stats</li>
                                    <li>All lineup optimizer features</li>
                                    <li>DFS performance tracker</li>
                                    <li>Analysis for every news item</li>
                                    <li>DFS picks and strategy articles</li>
                                </ul>
                                <a class="minutes-viz-wall__button" href="/subscribe/?refer=lineups-minutes">Get Instant Access</a>
                            </div>
                        `;

                        $vizContainer.append(paywall);

                    }, 1000);

                    return;
                }

                
                // Get the projected minutes
                const players = await $.ajax({
                    type   : 'GET',
                    url    : '/basketball/ajax/get-projected-minutes.php',
                    data   : { team },
                    timeout: 15000,
                });

                // Remove the loading screen
                $vizContainer.find('.minutes-viz-loader').remove();

                // Get the width of the minutes meter (varies based on screen size)
                const minutesMeterWidth = $('.minutes-viz').width() - 130 - 30; // 130 is player name width, 30 is padding

                // The scale of the minutes meter (max value)
                // Not set to 48 since players are rarely projected for more than 40 minutes
                const minutesScaleMax = 40;

                // Generates the HTML for each section of the minutes viz
                function generateSection(title, players) {
                    if (players.length === 0) { return ''; }

                    return `
                    <div>
                        <div class="minutes-viz__title">${title}</div>
                        <ul class="minutes-viz__players">
                            ${players.map(p => {
                                const projPct = (p.proj / minutesScaleMax) * 100;
                                const rangeStartPct = (p.min / minutesScaleMax) * 100;
                                const rangePct = ((p.max - p.min) / minutesScaleMax) * 100;
                                const hasInj = p.inj !== 'No';
                                const injIndicator = hasInj ? `<div class="minutes-viz__inj">${p.inj}</div>` : '';
                                const isZeroClass = p.proj === 0 ? ' is-zero' : '';
                                const isOutClass = p.inj === 'Out' ? ' is-out' : '';
                                const leftPos = p.proj === 0 ? '-1' : ((projPct/100) * minutesMeterWidth) - 9;
                                const startingLeftPos = p.proj === 0 ? '-9px' : '0';
                                const hasPlayed = rangePct > 0;
                                const nameCharacters = p.name.length;
                                const isLongName = nameCharacters > 21 || (nameCharacters > 15 && hasInj);
                                const playerName = isLongName ? `${p.firstname[0]}. ${p.lastname}` : p.name;

                                const meter = !hasPlayed ? '' : `<div class="minutes-meter__range" style="left: ${rangeStartPct}%;width: ${rangePct}%;"></div>`;
                                return `
                                <li class="minutes-viz__player">
                                    <div class="minutes-viz__player-name"><a href="${p.link}">${playerName}</a>${injIndicator}</div>
                                    <div class="minutes-meter">
                                        ${meter}
                                        <div data-left="${leftPos}" class="minutes-meter__proj${isZeroClass}${isOutClass}" style="left:${startingLeftPos};">${p.proj}</div>
                                    </div>
                                </li>`;
                            }).join('')}
                        </ul>
                    </div>`;
                }

                const starters = players.filter(p => p.lineup !== 'BE').sort((a, b) => b.proj - a.proj);
                const bench = players.filter(p => p.lineup === 'BE' && p.proj > 0).sort((a, b) => b.proj - a.proj);
                const mayNotPlay = players.filter(p => p.lineup === 'BE' && !['Out', 'OFS'].includes(p.inj) && p.proj === 0).sort((a, b) => b.max - a.max);
                const out = players.filter(p => p.lineup === 'BE' && p.inj === 'Out' && p.proj === 0).sort((a, b) => b.max - a.max);

                $vizContainer.append(`
                    <div class="minutes-viz__main">
                        ${generateSection('STARTERS', starters)}
                        ${generateSection('BENCH', bench)}
                        ${generateSection('MAY NOT PLAY', mayNotPlay)}
                        ${generateSection('OUT', out)}
                    </div>
                    <div class="minutes-viz__key">
                        <div class="minutes-viz__key-item">
                            <div class="minutes-meter__proj">#</div>
                            <div class="minutes-viz__key-label">Projected Minutes For Today</div>
                        </div>
                        <div class="minutes-viz__key-item" style="align-items: flex-start">
                            <div class="minutes-meter__range"></div>
                            <div class="minutes-viz__key-label">
                                <div>Typical Minutes Range</div>
                                <div class="minutes-viz__key-explain">Weighted average of last 10 team games. Wider bar = more variance.</div>
                            </div>
                    </div>
                `);

                requestAnimationFrame(() => {
                    $vizContainerMain = $vizContainer.find('.minutes-viz__main');
                    $vizContainerMain.find('.minutes-meter__proj').each(function(index, el){
                        const $proj = $(el);
                        const leftPos = $proj.data('left');
                        if (leftPos < 0) { return true; }
                        const timing = 100 + (index * 20);
                        $proj.css('transform', `translateX(${leftPos}px)`);
                        $proj.css('transition', `transform ${timing}ms ease-out`);
                    });

                    $vizContainerMain.find('.minutes-meter__range').each(function(index, el){
                        const $proj = $(el);
                        const timing = 350 + (index * 30);
                        $proj.css('opacity', 1);
                        $proj.css('transition', `opacity ${timing}ms ease`);
                    });
                });
            });
        });
    </script>
